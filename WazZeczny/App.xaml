<Application x:Class="WazZeczny.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WazZeczny"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/MenuButtonTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="BackgroundColor">#144177</SolidColorBrush>
            <SolidColorBrush x:Key="GridBackground">#312C40</SolidColorBrush>
            <SolidColorBrush x:Key="GridLineColor">#4F4867</SolidColorBrush>
            <SolidColorBrush x:Key="TextColor">#EBEBEB</SolidColorBrush>
            <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
            <FontFamily x:Key="MainFont">Assets/#Drain Sans Mono</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
